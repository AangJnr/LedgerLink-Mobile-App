<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@color/gery_background">

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/frmMSavF_top_control_bar">

    </RelativeLayout>

        <ListView
            android:id="@+id/lvwMSavFMembers"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/frmMSavF_top_control_bar"
            android:choiceMode="singleChoice"
            android:dividerHeight="2dp"
            android:elevation="2dp">

        </ListView>


    <org.applab.ledgerlink.fontutils.TypefaceTextView
        style="@style/RegularText"
        android:id="@+id/txtMSavFEmpty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/empty_members_list"
        android:layout_below="@id/frmMSavF_top_control_bar" />

</RelativeLayout>
